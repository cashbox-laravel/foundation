{
    "name": "cashier-provider/sber-online",
    "description": "Driver for online payments via Sber (see cashier-provider/core)",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "andrey-helldar",
        "helldar",
        "bank",
        "billing",
        "cashier",
        "sber",
        "online",
        "payment",
        "payments"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/cashier-provider/sber-online/issues",
        "source": "https://github.com/cashier-provider/sber-online"
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "cashier-provider/core": "^2.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "ext-json": "*",
        "dragon-code/support": "^5.0",
        "illuminate/database": "^6.0 || ^7.0 || ^8.0",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^4.0 || ^5.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "CashierProvider\\Sber\\Online\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
