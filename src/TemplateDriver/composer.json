{
    "name": "cashbox/bankname-technology",
    "description": "The Technology driver implements payments in the Bank billing system.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "your-name",
        "bank",
        "billing",
        "cashier"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro",
            "homepage": "https://github.com/andrey-helldar"
        }
    ],
    "support": {
        "issues": "https://github.com/cashbox-laravel/foundation/issues",
        "source": "https://github.com/cashbox-laravel/foundation"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/TheDragonCode"
        },
        {
            "type": "open_collective",
            "url": "https://opencollective.com/dragon-code"
        },
        {
            "type": "boosty",
            "url": "https://boosty.to/dragon-code"
        },
        {
            "type": "yoomoney",
            "url": "https://yoomoney.ru/to/410012608840929"
        }
    ],
    "require": {
        "php": "^8.1",
        "cashbox/bankname-auth": "^3.0",
        "cashbox/core": "^3.0",
        "psr/http-message": "^1.1"
    },
    "require-dev": {
        "ext-json": "*",
        "dragon-code/support": "^5.0",
        "illuminate/database": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0 || ^7.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^4.0 || ^5.0 || ^6.0"
    },
    "repositories": [
        {
            "type": "github",
            "url": "git@github.com:cashbox/driver-auth.git"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cashbox\\BankName\\Technology\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dragon-code/codestyler": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.x-dev"
        },
        "thanks": [
            {
                "name": "cashbox/core",
                "url": "https://github.com/cashbox-laravel/core"
            },
            {
                "name": "psr/http-message",
                "url": "https://github.com/php-fig/http-message"
            }
        ],
        "title": "Driver Template"
    }
